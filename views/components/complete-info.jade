.complete-info
    h1.title لطفا مشخصات فردی خود را کامل کنید
    form(action="/dashboard/complete-info" method="POST") 
        .overflow-hidden
            .part 
                .text-input
                    i.fa.fa-user 
                    input(type="text" name="firstName" placeholder="نام")
                .text-input
                    i.fa.fa-user 
                    input(type="text" name="lastName" placeholder="نام خانوادگی")
                .text-input
                    i.fa.fa-credit-card-alt
                    input(type="text" name="idNumber" placeholder="کد ملی")
            .part
                .text-input
                    i.fa.fa-envelope 
                    input(type="text" name="email" placeholder="پست الکترونیکی")
                .text-input
                    i.fa.fa-map-marker 
                    input(type="text" name="address" placeholder="نشانی")
        input.submit(type="submit" value="ثبت")
        
